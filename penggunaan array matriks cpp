#include <iostream>
#include <iomanip>
using namespace std;
int main(int argc, char *argv[]) {
 int i,j;
 int data[3][4];
 cout<<"-------------------------\n\n";
 for (i=0;i<3;i++)
 { for(j=0;j<4;j++)
 {cout<<"grademhs "<<setw(2)<<i<<";"<<j<<" = "<<setw(3);
 cin>>data[i][j];}}
 cout <<"            [0][1][2][3]\n";
 for (i=0;i<3;i++)
 {
 cout <<"grademhs["<<i <<"]";
 for(j=0;j<4;j++)
 { cout <<setw(3)<<data[i][j];}
 cout<<endl;}
 return 0;
}
