#include<iostream> 
#include<stdio.h>
#include<conio.h>
using namespace std;
main(){
	char nama[100];
	int p,i,n,g,o,hasil;
	cout<<"nama mahasiswa:	";cin.getline(nama,100);
	cout<<"masukan nilai1:	";cin>>p;
	cout<<"masukan nilai2:	";cin>>i;
	cout<<"masukan nilai3:	";cin>>n;
	cout<<"masukan nilai4:	";cin>>g;
	cout<<"masukan nilai5:	";cin>>o;
	cout<<"\n\n=============hasil akhir=======\n";
	cout<<"\nMahasiswa yang bernama : "<<nama;
	cout<<"\n\nmemperoleh nilai rata-rata tugas : ";
	hasil = (p+i+n+g+o)/5;
	cout<<hasil;
	getch();
	
}
